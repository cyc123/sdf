<!--pages/invite/record/record.wxml-->
<view class='container'>
  <view class="myui-info flexlb wt90" wx:for="{{info}}" wx:key>
    <view class='left flex' >
      <image src='{{item.avatar}}'></image>
      <text class='text'>{{item.nickname}}</text>
    </view>
    <view class='right'><text class='color1'>{{item.recharge_text}} </text> {{item.created_at}}</view>
  </view>
</view>
